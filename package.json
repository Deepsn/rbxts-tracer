{
  "name": "@rbxts/tracer",
  "version": "0.0.3",
  "description": "Raycast wrapper for Roblox",
  "main": "out/init.lua",
  "scripts": {
    "lint:check": "biome check .",
    "lint:fix": "biome check . --fix",
    "build": "rbxtsc",
    "watch": "rbxtsc -w",
    "test:watch": "rbxtsc --type=game -p test -i test/include -w",
    "test:serve": "rojo serve test/default.project.json",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Deepsn/rbxts-tracer.git"
  },
  "author": "Deepsn",
  "license": "ISC",
  "types": "out/index.d.ts",
  "files": [
    "out",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.2",
    "@rbxts/compiler-types": "2.3.0-types.1",
    "@rbxts/types": "^1.0.783",
    "@rbxts/testez": "^0.4.2-ts.0",
    "roblox-ts": "^2.3.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@rbxts/services": "^1.5.4"
  }
}
